<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>vip.vinyoung</groupId>
        <artifactId>turing</artifactId>
        <version>0.0.1</version>
    </parent>
    <groupId>vip.vinyoung</groupId>
    <artifactId>cache</artifactId>

    <properties>
        <spring-boot-starter-validation.version>3.1.0</spring-boot-starter-validation.version>
        <redisson-spring-boot-starter.version>3.21.3</redisson-spring-boot-starter.version>
        <lang3-version>3.12.0</lang3-version>
    </properties>

    <dependencies>
        <!-- redis -->
        <dependency>
            <groupId>org.redisson</groupId>
            <artifactId>redisson-spring-boot-starter</artifactId>
            <version>${redisson-spring-boot-starter.version}</version>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-validation</artifactId>
            <version>${spring-boot-starter-validation.version}</version>
        </dependency>
        <!-- utils -->
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
            <version>${lang3-version}</version>
        </dependency>
    </dependencies>

    <build>
        <finalName>cache</finalName>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
                <version>2.6.0</version>
                <configuration>
                    <mainClass>none</mainClass>
                    <classifier>execute</classifier>
                    <outputDirectory>../packages</outputDirectory>
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>
